!!!
html
	head
		title Personal Stock Monitor
		link(href="/stylesheets/style.css", rel="stylesheet")
		link(href="/stylesheets/layout.css", rel="stylesheet")
		link(href="/stylesheets/jquery-ui-1.9.1.custom.css", rel="stylesheet")
		block styles
		script(src="/javascripts/jquery-1.7.2.min.js")
		script(src="/javascripts/jquery-1.7.2.min.js")
		script(src="/javascripts/jquery-ui-1.9.1.custom.js")
		block scripts
	body
		#topbar
			#site-title-mini
				a(href="/") Personal Stock Monitor
			#search-div
				form(action="/search", method="get")
					input(type="text", placeholder="search...", name="q", autocomplete="off")
					button
						img(src="/images/search.png")
			#quick-links
				ul
					li
						a(href="/sell_stock").btn Sell Stocks
					li
						a(href="/home") Home
					li
						a(href="/profile") Profile
					li
						a(href="/signout") Sign Out
		#maincontent
			block body
